package com.edu.loop.test;

public class BasicLoopTest1 {

	public static void main(String[] args) {
		System.out.println("==========for=========");
		for (int i = 0 ; i < 10 ; i ++) {
			System.out.println(" i : " + i);
		}
		
		
		System.out.println("==while==");
		int i = 0;
		while ( i < 10) {
			System.out.println("i : " + i);
			i ++;
		}

	}

}
