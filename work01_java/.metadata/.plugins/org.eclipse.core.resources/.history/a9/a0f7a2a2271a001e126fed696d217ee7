package com.edu.datatype;
/*
 * 오전에 정리한 자바의 DataType을 이해할 수 있는 클래스 작성
 * 2개의 클래스를 한번에 작성할 때는 
 * 1. main()을 가지고 있는 클래스 앞에만 public을 지정
 * 2. public으로 지정된 클래스 명으로 java file이 저장되어야 한다
 */

class WhileBoard{
	// 1. 필드 선언
	String madeCompany;
	float price;
	boolean scratch;
	char size;
	// 2. 필드 주입 기능 정의 (void | 인자값)
	public void setWhiteBoard(String madeCompany, float price, boolean scratch, char size) {
		this.madeCompany = madeCompany;
		this.price = price;
		this.scratch = scratch;
		this.size = size;
	}
	// 3. 주입된 필드 값을 반환(??? | 인자값 X)
	public String getWhiteBoard() {
		return madeCompany + " , " + price + " , " + scratch + " , " + size;
	}
}

public class WhiteBoardTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
